<UserControl x:Class="NPSim.Views.PhysicalMediaUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NPSim.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Canvas>
        <Line Name="PhysicalMediaCanvasElement" Stroke="Black"
          X1="{Binding From.X}" Y1="{Binding From.Y}"
          X2="{Binding To.X}" Y2="{Binding To.Y}"/>
    </Canvas>
</UserControl>
